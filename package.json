  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "html-loader": "^1.1.0",
    "http-server": "^0.12.3",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "js-cookie": "^3.0.0",
    "uuid": "^8.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/bourbonltd/gist-web.git"
  },
  "keywords": [
    "gist",
    "in-app",
    "messages",
    "product",
    "messaging",
    "in-product",
    "gist.build"
  ],
  "bugs": {
    "url": "https://gitlab.com/bourbonltd/gist-web/issues"
