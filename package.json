    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "uuid": "^8.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/customerio/gist-web.git"
  },
  "keywords": [
    "gist",
    "in-app",
    "messages",
    "product",
    "messaging",
    "in-product",
    "gist.build",
    "customerio"
